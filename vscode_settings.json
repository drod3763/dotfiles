		"plaintext": true,
		"markdown": false
	},
	"go-template.patterns": ["*.tmpl"],
	"search.exclude": {
		"**/node_modules": true,
		"**/bower_components": true,
		"**/env": true,
		"**/venv": true
	},
	"files.watcherExclude": {
		"**/.git/objects/**": true,
		"**/.git/subtree-cache/**": true,
		"**/node_modules/**": true,
		"**/env/**": true,
		"**/venv/**": true,
		"env-*": true
	},
