			"path": "bash"
		},
		"zsh": {
			"path": "zsh"
		},
		"fish": {
			"path": "fish"
		},
		"tmux": {
			"path": "tmux",
			"icon": "terminal-tmux"
		},
		"pwsh": {
			"path": "pwsh",
			"icon": "terminal-powershell"
		},
		"flatpak": {
			"path": "flatpak-spawn",
			"args": ["--host", "zsh"],
			"icon": "star"
		}
	},
